{% extends 'layout.html.twig' %}

{% block titre %}Ayurveda Rhône-alpes, Bien être, relaxation.{% endblock %}
{% block content %}
    {% for message in app.session.getFlashBag.get('message') %}
        <h1>Oup's !</h1>
        <p>{{ message }}</p>
        <hr/>
        <p>Utilisez le menu à gauche pour naviguer sur le site,<br/>
